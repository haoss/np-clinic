doctype

html(lang="ru")
  
  -var active = 'Контакты' //активный пункт меню, список в header.jade
  -var pageTitle = 'Наши контакты' + ' - НП-Клиника' //title страницы для генерации верстки  

  include ./_include/_head

  body
    
    // Preloader
    .loader
      .loader__block
    
    include ./_include/_header
    
    // Content
    .content.content--all
      .container
        
        // Breadcrumbs
        ol.breadcrumb
          li
            a(href='#') НП-клиника 
          li.active Контакты
        
        .row
          // Center collumn
          .col-sm-8
            
            .section__header.section__header--left
              .h1 Наши контакты
            
            h4 Далеко-далеко за словесными горами в стране гласных и согласных живут рыбные тексты. Вдали от всех живут они в буквенных домах на берегу Семантика большого языкового океана. 
            
            .contacts__row
              p 
                i.ion-ios-location 
                | Москва, Каширское шоссе, дом 58 корпус 3
                br
                i.ion-android-call
                | 8 (964) 539 07 79
                br
                i.ion-ios-email
                a(href="mailto:help@np-clinic.ru").red help@np-clinic.ru
            
            #map_canvas
            // Google Map
            script(src='https://maps.googleapis.com/maps/api/js?sensor=false')
            script.
              function initialize() {
                var myLatlng = new google.maps.LatLng(55.644359,37.6683403);
                var map_options = {
                  zoom: 16,
                  mapTypeId: google.maps.MapTypeId.ROADMAP,
                  center: myLatlng,
                  scrollwheel: false,
                  disableDefaultUI: true
                }
                var map = new google.maps.Map(document.getElementById('map_canvas'), map_options)
                var myIcon = new google.maps.MarkerImage('./img/google-map-icon.png', null, null, null, new google.maps.Size(125,87));
                var marker = new google.maps.Marker({
                  position: myLatlng,
                  map: map,
                  icon: myIcon
                });
                /* Remove if you dont want B/W Google Map */
                /*
                var styles = [
                {
                featureType: "all",
                stylers: [
                { saturation: -100 }
                ]
                }
                ];
                map.setOptions({styles: styles});
                */
              }
              google.maps.event.addDomListener(window, 'load', initialize);
            // Google Map Ends!

            //-.google-map
              iframe( src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d6368.494477338286!2d37.66759205480665!3d55.64372351468678!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x414ab3a1e4e40539%3A0x863144f94adf11ef!2z0JrQsNGI0LjRgNGB0LrQvtC1INGILiwgNTjQujMsINCc0L7RgdC60LLQsCwg0KDQvtGB0YHQuNGPLCAxMTU0MDk!5e0!3m2!1sru!2sua!4v1467113432998" width="600" height="450" frameborder="0" allowfullscreen)
            
            p Далеко-далеко за словесными горами в стране гласных и согласных живут рыбные тексты. Вдали от всех живут они в буквенных домах на берегу Семантика большого языкового океана. 
          
          // Right collumn
          .col-sm-4
            
            include _include/_right_collumn
    
    include ./_include/_footer
